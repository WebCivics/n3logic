{
  "compilerOptions": {
    "target": "ES2020",
  "module": "node16",
    "declaration": false,
    "outDir": "./dist-test",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["es2020"],
    "types": ["node", "jest"],
    "noEmit": true
  },
  "include": ["n3logic/**/*.ts", "tests/**/*.ts"],
  "exclude": ["node_modules", "dist", "dist-test"]
}
